{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"        
      ]
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "ratings",
      "databaseName": "bfyoc",
      "collectionName": "ratings",
      "id" : "{ratingId}",
      "partitionKey": "{ratingId}",
      "connectionStringSetting": "AzureCosmosDBConnectionString"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
